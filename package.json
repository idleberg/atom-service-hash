{
  "name": "service-hash",
  "version": "0.1.0",
  "description": "Provides hashing algorithms through a web worker",
  "private": false,
  "main": "./lib/main.js",
  "typings": "./types",
  "scripts": {
    "build": "npm run clean && node ./tools/build.mjs",
    "clean": "rimraf ./lib",
    "format": "prettier ./src/**/*.ts --write",
    "lint:ts": "eslint ./src --ignore-path .gitignore",
    "lint": "npm-run-all lint:*",
    "prepublishOnly": "npm run build",
    "start": "cross-env ESBUILD_WATCH=true node ./tools/build.mjs",
    "test": "echo 'Skipping, no tests specified';exit 0;"
  },
  "keywords": [
    "hash",
    "hashing",
    "web worker",
    "web workers",
    "crc32",
    "md4",
    "md5",
    "ripemd-160",
    "sha-1",
    "sha-3",
    "sha-224",
    "sha-256",
    "sha-384",
    "sha-512"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/atom-service-hash"
  },
  "homepage": "https://atom.io/packages/service-hash",
  "bugs": {
    "url": "https://github.com/idleberg/atom-service-hash/issues"
  },
  "license": "MIT",
  "engines": {
    "atom": ">=1.52.0 <2.0.0"
  },
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "providedServices": {
    "service-hash": {
      "description": "Provides hashing algorithms through a web worker",
      "versions": {
        "0.1.0": "provideHash"
      }
    }
  },
  "devDependencies": {
    "@atxm/developer-console": "^0.5.0",
    "@types/atom": "^1.40.5",
    "@types/node": "^14.14.43",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.12.9",
    "eslint": "^7.25.0",
    "eslint-plugin-json": "^2.1.2",
    "hash-wasm": "^4.8.0",
    "husky": ">=4 <5",
    "jsonlint": "^1.6.3",
    "lint-staged": "^10.5.3",
    "log-symbols": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "source-map-explorer": "^2.5.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3",
    "uuid": "^8.3.2"
  },
  "remarkConfig": {
    "plugins": [
      "@stylelint/remark-preset"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "volta": {
    "node": "14.17.1",
    "yarn": "1.22.10"
  }
}
